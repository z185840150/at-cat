{
  "presets": [
    "es2015",
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-0"
  ],
  "plugins": [
    ["transform-decorators-legacy"],
    ["transform-class-properties"],
    ["transform-builtin-extend", {
      "globals": ["Error", "Array", "Map"]
    }]
  ]
}